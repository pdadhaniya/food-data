.wrapper {
  background: url('<%=asset_path("Chicago_Skyline.jpg") %>') no-repeat;
  background-size: cover;
  height: 100vh;
}

.panel {
  background-color: #E1E1E1;
  opacity: 0.9;
}

.about {
  width: 100%;
  background-color: #6BB4B5;
  opacity: 0.95;
}

.about-box {
  color: #FFFFFF;
  padding: 10px;
}

.about-box p {
  font-size: 24px;
  font-family: "Palatino Linotype", "Book Antiqua", "Palatino", serif;
  font-weight: 200;
}

.button-holder {
  text-align: center;
}

.more {
  border-radius: 5px;
}